{
  "name": "@quailjs/quail-cli",
  "description": "The Quail command line interface",
  "version": "0.0.7",
  "author": "Jesse Beach <jesse.r.beach@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jessebeach/quail-cli.git"
  },
  "bugs": "http://github.com/jessebeach/quail-cli/issues",
  "dependencies": {
    "commander": "^2.9.0",
    "core-js": "^0.9.13",
    "jquery": "^2.1.4",
    "opener": "^1.4.1"
  },
  "scripts": {
    "compile": "babel -e -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "jscs -c .jscsrc src bin && eslint -c .eslintrc src bin",
    "watch": "npm-watch"
  },
  "bin": {
    "quail": "bin/quail"
  },
  "preferGlobal": true,
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^4.1.6",
    "eslint": "^1.10.1",
    "jscs": "jscs-dev/node-jscs#c5adeba",
    "npm-watch": "0.0.1"
  },
  "files": [
    "bin",
    "lib"
  ],
  "watch": {
    "compile": "src/*.js"
  }
}
